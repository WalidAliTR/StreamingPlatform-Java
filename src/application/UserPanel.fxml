<AnchorPane fx:id="formpanel" blendMode="MULTIPLY" minHeight="361.0" minWidth="281.0" prefHeight="361.0" prefWidth="281.0" style="-fx-text-fill: WHITE; -fx-background-color: #e7eaf6;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.UserPanelController">
   <children>
      <TextField fx:id="Name" layoutX="47.0" layoutY="53.0" prefHeight="26.0" prefWidth="192.0" promptText="User Name">
