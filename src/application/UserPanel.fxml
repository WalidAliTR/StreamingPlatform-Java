            <Font size="15.0" />
         </font></TextField>
      <PasswordField fx:id="UserPassword" layoutX="47.0" layoutY="114.0" prefHeight="26.0" prefWidth="192.0" promptText="The Current Password">
         <font>
            <Font size="15.0" />
         </font>
      </PasswordField>
      <PasswordField fx:id="NewPassword" layoutX="47.0" layoutY="173.0" prefHeight="26.0" prefWidth="192.0" promptText="The New Password">
